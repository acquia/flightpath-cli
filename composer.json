{
    "name": "acquia/ama-report-builder",
    "description": "Acquia migrate assessment report builder.",
    "type": "project",
    "require": {
        "drutiny/acquia": "^4.0",
        "drutiny/drutiny": "^3.3.1",
        "drutiny/pantheon": "^1.1"
    },
    "authors": [
        {
            "name": "Josh Waihi",
            "email": "josh.waihi@acquia.com"
        }
    ],
    "minimum-stability": "dev",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "Drutiny\\Acquia\\AMA\\": "src/",
            "DrutinyTests\\Acquia\\AMA\\": "tests/src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/drutiny/plugin-acquia.git"
        }
    ],
    "scripts": {
      "build": [
        "./vendor/bin/build_phar `git branch | awk '{print $2}'` --name=flightpath"
      ]
    }
}
